<p><%= message %></p>
<p class="citeAuthor">&mdash; <a href="mailto:<%= author.email %>"><%= author.name %></a> @ <%= author.date %></p>
<form action="#" class="hidden js-commitMetainfo">
  <input type="hidden" name="sha" value="<%= sha %>">
  <input type="hidden" name="tree" value="<%= tree.sha %>">
  <% if (typeof head !== 'undefined') { %>
    <input type="hidden" name="head" value="<%= head %>">
  <% } %>
</form>